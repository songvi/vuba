{
    "name": "vuba/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework with doctrine ORM and bootstrap config.",
    "keywords": ["silex", "skeleton", "bootstrap", "doctrine", "orm", "database", "extension", "provider"],
    "license": "MIT",
    "homepage": "https://github.com/fredjuvaux/silex-orm-skeleton",
    "authors": [
        { 
            "name": "Fred Juvaux",
            "homepage": "http://fredjuvaux.fr/",
            "email": "hello@fredjuvaux.fr" 
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.9.9",
        "silex/silex": "~2.0",
        "silex/web-profiler": "~2.0",
        "symfony/browser-kit": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/form": "2.5.2",
        "symfony/monolog-bridge": "~2.3",
        "symfony/process": "~2.3",
        "symfony/security": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "2.5.2",
        "symfony/serializer": "2.5.2",
        "gigablah/silex-oauth": "~2.0@dev",
        "moust/silex-cache": "dev-master",
        "dflydev/doctrine-orm-service-provider": "~2.0@dev",
        "saxulum/saxulum-validator-provider": "~2.0@dev",
        "wixel/gump": "dev-master",
        "ramsey/uuid":"~3.6",
        "php-amqplib/php-amqplib": "2.6.*",
        "mrprompt/silex-uuid" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
